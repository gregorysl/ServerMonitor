{
  "name": "server-monitor",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "antd": "^3.13.6",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "customize-cra": "^0.2.12",
    "dateformat": "^3.0.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "filesize": "^4.1.2",
    "final-form": "4.8.1",
    "final-form-arrays": "1.1.2",
    "node-sass": "^4.10.0",
    "prettier": "^1.16.4",
    "react": "^16.8.3",
    "react-app-rewired": "^2.0.3",
    "react-dom": "^16.8.3",
    "react-final-form": "4.1.0",
    "react-final-form-arrays": "2.0.3",
    "react-page-visibility": "^3.2.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-table": "^6.9.2",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "global-require": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "linebreak-style": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "react/no-unused-prop-types": "warn",
      "no-unused-vars": "warn",
      "no-debugger": 0,
      "no-console": 0,
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
